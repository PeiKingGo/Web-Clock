{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "standard",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "camelcase": "off",
        "semi": ["error", "always"],
        "indent": ["error", 4],
        "no-tabs": "off"
    }
}
